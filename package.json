{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "tailwind": "tailwindcss --jit -w -i resources/css/tailwind.css -o public/css/style.css",
        "tailwind:prod": "cross-env NODE_ENV=production tailwindcss --jit -m -i resources/css/tailwind.css -o public/css/style.css"
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.3.4",
        "@tailwindcss/line-clamp": "^0.2.2",
        "@tailwindcss/typography": "^0.4.1",
        "autoprefixer": "^10.4.0",
        "axios": "^0.21",
        "bootstrap": "^4.6.0",
        "cross-env": "^7.0.3",
        "jquery": "^3.6",
        "laravel-mix": "^6.0.19",
        "lodash": "^4.17.19",
        "popper.js": "^1.16.1",
        "postcss": "^8.4.4",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "tailwindcss": "^2.2.19"
    }
}
